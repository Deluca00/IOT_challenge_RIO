[req]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
x509_extensions    = v3_req

[dn]
CN = ACHI

[v3_req]
subjectAltName = @alt_names

[alt_names]
DNS.1   = ACHI
DNS.2   = ACHI.local
DNS.3   = localhost
IP.1    = 127.0.0.1
IP.2    = 192.168.137.86
